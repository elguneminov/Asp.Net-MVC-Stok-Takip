@model MVCProje_MuratYucedag.Models.Entity.TBLMusteriler
@{
    ViewBag.Title = "MusteriGuncelle";
    Layout = "~/Views/Shared/_MainLayout.cshtml";
}

@using (Html.BeginForm())
{
    <div class="form-group">
        <div>
            @Html.LabelFor(x => x.MusteriId, new { @class = "text-muted" })
        </div>
        <div>
            @Html.TextBoxFor(x => x.MusteriId, new { @class = "form-control" })
        </div>
    </div>

    <div class="form-group">
        <div>
            @Html.LabelFor(x => x.MusteriAd, new { @class = "text-muted" })
        </div>
        <div>
            @Html.TextBoxFor(x => x.MusteriAd, new { @class = "form-control" })
        </div>
    </div>

    <div class="form-group">
        <div>
            @Html.LabelFor(x => x.MusteriSoyad, new { @class = "text-muted" })
        </div>
        <div>
            @Html.TextBoxFor(x => x.MusteriSoyad, new { @class = "form-control" })
        </div>
    </div>

    <div class="form-group">
        <button class="btn btn-primary">Müşteri Güncelle</button>
    </div>
}

